#!/usr/bin/perl

use lib "/httpd/modules";
use ZOOVY;
use CGI;
use ZWEBSITE;

use Data::Dumper;
my $q = new CGI;


## sessiontkt --
##		

open F, ">/tmp/qbms";
print F Dumper($q);
close F;

my $USERNAME = $q->param('appdata');
my $APPID = $q->param('appid');
my $ConnectionTicket = $q->param('conntkt');

&ZWEBSITE::save_website_attrib($USERNAME,'qbms_conntkt',$ConnectionTicket);
&ZWEBSITE::save_website_attrib($USERNAME,'qbms_appid',$APPID);

print STDERR Data::Dumper($q);

print "Content-type: text/plain\n\n";
print "Thank you, come again!\n";



