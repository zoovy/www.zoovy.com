<tr>
	<td colspan='2' style='background:url(/includes/header-20120404/images/menu_sublinks_bg-1x28.gif) repeat-x; height:24px;' valign='top'>
	<div style='position:relative; height:24px;'>

		<div style='position:absolute; left:-18px; top:0;'><img src='/includes/header-20120404/images/submenu_endcap_left-18x32.gif' width='18' height='32' alt='' /></div>
	
		<div style='position:absolute; right:-18px; top:0;'><img src='/includes/header-20120404/images/submenu_endcap_right-18x32.gif' width='18' height='32' alt='' /></div>
<!-- the width needs to be specified on this div because otherwise IE 7 blows chunks. there's a shocker -->	
		<div style='position:absolute; left:37px; top:0px; height:24px; width:905px;' id='subcats'>");
	my @focus_nav;
	my @navdata = split('\|', $nav);

	my $i = 0;
## rather than load 256 million background images, we load 1 image and only show a portion of it
## using css. should be faster because the extra code for it is minimal.
	my $infocus; ## changes between on and off to set whether the on or off values are set for hover/bg image
	print("
<!--
focusTab = $navdata[0]
focusPage = $navdata[1]
-->
");
	
	if($navdata[0] eq 'products'){
		@focus_nav = @products_nav;
		}
	elsif($navdata[0] eq 'services'){
		@focus_nav = @services_nav;
		}
	elsif($navdata[0] eq 'clients'){
		@focus_nav = @clients_nav;
		}
	elsif($navdata[0] eq 'company'){
		@focus_nav = @company_nav;
		}
	elsif($navdata[0] eq 'news'){
		@focus_nav = @news_nav;
		}
	else{
## no nav is in focus. output a blank bar.
	}


##loop through the array of pages and output the link and icon for each.
	foreach my $navref (@focus_nav) {
		if($navdata[1] eq $navref->{'pagename'}){
			$infocus = 'on';
			}
		else	{
			$infocus = 'off'
			}
		print("<div class='submenu_$infocus submenu_$navref->{'pagename'}'></div>");
		print("<div class='catlink_$infocus' style='float:left;'><a href=\"$navref->{'link'}\">$navref->{'pretty'}</a></div>");

		$i++;
		}

		print("</div>
		
	</div>
	</td>
</tr>
<!-- shading for grey horizontal bar beneath nav -->
<tr>
	<td style='background:#7f7f7f; height:4px;'></td>
	<td style='background:#787879; height:4px;'></td>
</tr>");
	}
